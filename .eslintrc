{
	"extends": ["eslint-config-airbnb"],
  "rules": {
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "one-var": "off",
    "func-names": ["error", "never"],
    "no-console": "off",
    "comma-dangle": "off",
    "class-methods-use-this": "off"
  },
  "globals": {
    "window": true,
    "document": true,
    "createjs": true
  }
}
